<form id="addCarForm" method="POST" action="{{ path('add_car') }}" enctype="multipart/form-data">
{{ form_start(form) }}
    {{ form_row(form.brand) }}
    {{ form_row(form.model) }}
    {{ form_row(form.color) }}
    {{ form_row(form.image) }}
    {{ form_row(form.km) }}
    {{ form_row(form.price) }}
    {{ form_row(form.available) }}
    <button type="submit">Add Car</button>
{{ form_end(form) }}
</form>
